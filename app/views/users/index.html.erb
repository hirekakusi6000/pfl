  <%= search_form_for @q do |f| %>
    <div class="input_group_p">
      <div class="input-group">
        <%= f.search_field :name_cont, class: "form-control",placeholder:"ユーザー検索"%>
      </div>
    </div>
  <% end %>

  <div class="index_icon_pp">
    <% @users.each do |user| %>
      <div class="index_icon_p" ontouchstart="">
        <a href="/users/<%= user.id %>"><img src="<%= user.icon_image %>"></a>
        <p  class="index_user_name_p"><a class="index_user_name" href="/user/<%= user.id %>/pfl"><%= user.name %></a></p>
      </div>
    <% end %>
  </div>

  <div class="paginate">
    <%= paginate @users %>
  </div>
