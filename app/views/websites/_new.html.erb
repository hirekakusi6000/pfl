 <!-- ポップアップ Twitter追加-->
<%= form_with(model: @website, local: true) do |form| %>
  <div class="popup_wrap">
    <input id="trigger_twitter" type="checkbox">
    <div class="popup_overlay">
      <label for="trigger_twitter" class="popup_trigger"></label>
      <div class="popup_content_p">
      <div class="popup_content add_sns_popup">
        <p class="popup_title" style="margin-bottom:27px;">Twitterリンク追加</p>
        <div class="sns_acount_form_pp" style="height:80px;">
           <div class="sns_acount_form_p">
            <label class="form_title">アカウントのID&emsp;</label>
            <%= form.text_field :account,class:"sns_edit_form",maxlength: "100",required: true%>
            <label class="form_title">コメントを追加&emsp;</label>
            <%= form.text_field :comment,class:"sns_edit_form",maxlength: "100"%>
         </div>
      </div>
      <p><label class="edit_close_btn"  for="trigger_twitter"><i class="fas fa-times-circle edit_close_btn"></i></label></p>
      <input class="OK" type="submit" value="保存" name="add_twitter">
     </div>
    </div>
   </div>
  </div>
 <% end %>

